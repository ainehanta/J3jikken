#include "h8-3052-iodef.h"

#define _WAIT_LOOP 17400

void wait_ms(int);

int main(void)
{
  P9DDR = 0x30;  /* ポート9の初期化(P94--P95を出力に設定) */
  int i = 12;

  while(1)
  {
    P9DR = 0x10;
    wait_ms(i);
    P9DR = 0x30;
    wait_ms(i);
  }
}

void wait_ms(int ms)
{
  int i;
  for(;ms>0;ms--)
  for(i=0;i<_WAIT_LOOP;i++);
}
